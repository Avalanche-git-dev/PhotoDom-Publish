{
  "name": "photo-dom-2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --configuration production",
    "build:ssr": "ng build && ng run photo-dom-2:server",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:photo-dom-2": "node dist/photo-dom-2/server/server.mjs",
    "api-gen:user": "ng-openapi-gen --input ./src/openapi/user-service-openapi.json --output ./src/services/user-service/",
    "api-gen:photo": "ng-openapi-gen --input ./src/openapi/photo-service-openapi.json --output ./src/services/photo-service/",
    "api-gen:comment": "ng-openapi-gen --input ./src/openapi/comment-service-openapi.json --output ./src/services/comment-service/",
    "api-gen:all": "npm run api-gen:user && npm run api-gen:photo && npm run api-gen:comment"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.4",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.4",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/ssr": "^19.0.5",
    "@stomp/stompjs": "^7.0.0",
    "bootstrap": "^5.3.3",
    "buffer": "^6.0.3",
    "express": "^4.18.2",
    "jwt-decode": "^4.0.0",
    "ng-openapi-gen": "^0.52.0",
    "process": "^0.11.10",
    "rxjs": "~7.8.0",
    "sockjs-client": "^1.6.1",
    "tslib": "^2.3.0",
    "websocket": "^1.0.35",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.5",
    "@angular/cli": "^19.0.5",
    "@angular/compiler-cli": "^19.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@types/sockjs-client": "^1.5.4",
    "@types/websocket": "^1.0.10",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.6.2"
  }
}
